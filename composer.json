{
  "require": {
    "carlcs/craft-redactorcustomstyles": "3.0.4",
    "carlcs/craft-uielementfields": "1.0.0",
    "craftcms/aws-s3": "1.2.11",
    "craftcms/cms": "^3.6.4",
    "craftcms/contact-form": "^2.2",
    "craftcms/redactor": "2.8.7",
    "ether/tags": "1.0.7",
    "fruitstudios/linkit": "1.1.12.1",
    "hybridinteractive/craft-contact-form-extensions": "^1.2",
    "nystudio107/craft-seomatic": "3.3.42",
    "servd/craft-asset-storage": "2.5.1",
    "spicyweb/craft-neo": "2.9.10",
    "stenvdb/craft-download-assets": "1.0.2",
    "verbb/field-manager": "2.2.3",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": false,
      "craftcms/plugin-installer": false
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
